public class Palindrom{
	public static boolean isPalindrom(int val){
	int t = val;
	int s = 0;
	
	while(t > 0){
	s = s * 10 + t % 10;
	t = t / 10;
	}
	if(s == val){
	return true;
	}
return false;
}

public static void main(String[] args){
System.out.println(Palindrom.isPalindrom(12321));
System.out.println(Palindrom.isPalindrom(123));
}
}
